<!DOCTYPE html>
<html>

<head>
  <title>Search Results</title>
</head>

<body>
  <h1>Search Results</h1>
  <main>
    <header></header>
    <a href="/search">Search for a book</a>
    <a href="/">Take me to my library</a>
    <div></div>
    <div>
      <ul>
        <% allBooks.forEach( book => { %>
          <%- include("../../layouts/detail",{book:book} ) %>
        <% }) %>
      </ul>
    </div>
    <%- include("../../layouts/updateform", { formAction: '', shelves: shelves } ) %>
    <footer></footer>
  </main>

  <script src="https://code.jquery.com/jquery-3.3.1.min.js"></script>
  <script src="/scripts/selectbook.js"></script>

</body>

</html>