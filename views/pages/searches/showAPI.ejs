<!DOCTYPE html>
<html>

<head>
  <title>Search Results</title>
</head>

<body>
  <h1>Search Results</h1>
  <main>
    <header></header>
    <div></div>
    <div>
      <ul>
        <% allBooks.forEach( book => { %>
          <%- include("../../layouts/detail",{book:book, path_folder: "search", path_id: book.isbn, buttonname:"Select This Book"}) %>
        <% }) %>
      </ul>
    </div>
    <footer></footer>
  </main>
</body>

</html>